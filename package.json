{
  "name": "infrasim",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"rm -rf .next\" \"yarn ollama:start\" \"yarn next:dev\"",
    "next:dev": "wait-on http://localhost:11434 && next dev",
    "build": "next build",
    "start": "concurrently \"yarn ollama:start\" \"yarn next:start\"",
    "next:start": "wait-on http://localhost:11434 && next start",
    "lint": "next lint",
    "ollama:start": "ollama serve",
    "ollama:pull": "ollama pull llama3.2:latest",
    "ollama:setup": "yarn ollama:pull",
    "setup": "yarn install && yarn ollama:setup",
    "seed:companies": "tsx seed/populate-companies.ts",
    "seed": "yarn seed:companies",
    "reset-vector-db": "tsx reset-vector-db.ts",
    "reset-vector-db:force": "tsx reset-vector-db.ts --force",
    "reset-vector-db:quiet": "tsx reset-vector-db.ts --quiet",
    "clean": "rm -rf .next node_modules && echo 'Cleaned build artifacts and node_modules'"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@langchain/anthropic": "^0.3.21",
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "@types/d3": "^7.4.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.8",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "d3": "^7.8.0",
    "elkjs": "^0.10.0",
    "faiss-node": "^0.5.1",
    "langchain": "^0.0.190",
    "lodash": "^4.17.0",
    "lucide-react": "^0.514.0",
    "next": "^14.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pickleparser": "^0.2.1",
    "postcss": "^8.4.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "reactflow": "^11.10.0",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "uuid": "^9.0.1",
    "yaml": "^2.3.0",
    "zod": "^3.25.56",
    "zod-to-json-schema": "^3.24.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "tsx": "^4.19.4",
    "wait-on": "^8.0.3"
  }
}
